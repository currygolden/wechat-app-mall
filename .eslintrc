{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["html"],
  "globals": {
    "Page": false,
    "Component": false,
    "wx": false,
    "App": false,
    "getApp": false,
    "getCurrentPages": false,
    "__wxConfig": false,
    "mmp": false
  },
  "rules": {
    "prefer-const": 0,
    "prettier/prettier": 1,
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "space-before-function-paren": 0,
    "camelcase": 1,
    "eqeqeq": 2,
    "no-debugger": 1,
    "standard/computed-property-even-spacing": 0,
    "no-console": ["error", { "allow": ["warn", "error", "log"] }]
  }
}
